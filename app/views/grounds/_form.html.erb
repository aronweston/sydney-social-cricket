<%= form_for @ground do |f| %>
  
  <%= f.label :name, "Ground Name" %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>
  
  <%= f.label :image %>
  <%= f.text_field :image %>
  
  <%= f.label :google_map %>
  <%= f.text_field :google_map %>

    <%= f.submit %>
<% end %>