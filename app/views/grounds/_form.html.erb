<%= form_for @ground, :html => {:multipart => true} do |f| %>
  
  <%= f.label :name, "Ground Name" %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>
  
  <%= f.label :image, "Ground Image" %>
  <%= f.cl_image_upload :image %>
  
  <%= f.label :google_map %>
  <%= f.text_field :google_map %>

  <%= f.submit %>
<% end %>